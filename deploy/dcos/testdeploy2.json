{
  "id": "/arcite-cluster",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 10204,
        "name": "http"
      },
      {
        "containerPort": 2551,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 10205,
        "name": "remoting"
      },
      {
        "containerPort": 19999,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 10206,
        "name": "akkamgmthttp"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "nexus-docker.idorsia.com/arcite/arcite-core:1.86.46",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 0.25,
  "disk": 0,
  "env": {
    "LAUNCH": "CLUSTER_BACKEND",
    "ARCITE_CONF": "cluster1.conf"
  },
  "fetch": [
    {
      "uri": "file:///etc/docker.tar.gz",
      "extract": true,
      "executable": false,
      "cache": false
    }
  ],
  "instances": 2,
  "labels": {
    "ACTOR_SYSTEM_NAME": "marathon-api-docker-app-label"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 512,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0.5
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 300,
    "expungeAfterSeconds": 600
  },
  "healthChecks": [],
  "constraints": []
}


